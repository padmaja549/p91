#include<stdio.h>
#include<conio.h>
void main()
{
    char string1[80], string2[80];
    int i;
    printf("Enter a stringn");
    printf("?");
    scanf("%s",string2);
    for(i=0;string2[i]!='';i++)
        string1[i]=string2[i];
    string1[i]='';
    printf("n");
    printf("%sn",string1);
    printf("Number of characters=%dn",i);
}
